{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    {
      "src": "/admin/firebase-messaging-sw.js",
      "dest": "/admin/firebase-messaging-sw.js",
      "headers": {
        "Content-Type": "application/javascript",
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/admin",
      "dest": "/admin/index.html"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin/index.html"
    },
    {
      "src": "/admin/logo.svg",
      "dest": "/admin/logo.svg",
      "headers": {
        "Content-Type": "image/svg+xml"
      }
    }
  ]
}